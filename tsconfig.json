{
    "compilerOptions": {
        "baseUrl": "./resources",
        "paths": {
            "@/*": ["js/*"]
        },
        "target": "es5",
        "module": "commonjs",
        "jsx": "react",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "allowJs": true
    },
    "exclude": ["node_modules"]
}
